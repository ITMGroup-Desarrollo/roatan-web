---
import Layout from "../../layouts/Layout.astro";
import Header from "../../components/Headers/HeaderBlog.astro";
import Footer from "../../components/Footer/Footer.astro";
import Menu from "../../components/Headers/Menu.astro";
import Contacto from "../../components/contact/Contact.astro"
export function getStaticPaths() {
  return [{ params: { lang: "es" } }, { params: { lang: "en" } }];
}
const lang = Astro.params.lang || "es";
---

<Layout lang={lang}>  
  <Menu lang={lang} />
  <Header
    lang={lang}
    headerTitle=""
    headerUrl="header-faqs.webp"
    id="headerFaqs-container"
  />
  <Contacto lang={lang}/>
  <Footer lang={lang} videoFile="fondo-footer-gris.mp4"/>
  <style>body{
  background-color: var(--redSocial-color);
  
}</style>
</Layout>
