---
import Layout from "../../layouts/Layout.astro";
import Header from "../../components/Headers/Header.astro";
import Footer from "../../components/Footer/Footer.astro";
import Menu from "../../components/Headers/Menu.astro";
import Bienvenida from "../../components/Home/Bienvenida.astro";
import PortExperience from "../../components/Home/Port.astro";
import Travel from "../../components/Home/Travel.astro";
import Discover from "../../components/Home/Discover.astro";
import Trip from "../../components/Home/Trip.astro";
import Faqs from "../../components/Home/Faqs.astro";
export const prerender = true;
export function getStaticPaths() {
  return [{ params: { lang: "es" } }, { params: { lang: "en" } }];
}
const lang = Astro.params.lang || "es";
---

<Layout lang={lang}>  
  <Menu lang={lang} />
  <Header lang={lang} />
  <Bienvenida lang={lang} />
  <PortExperience lang={lang} />
  <Travel lang={lang} />
  <Discover lang={lang} />
  <Trip lang={lang} />
  <Faqs lang={lang} />
  <Footer lang={lang} videoFile="fondo-footer-gris.mp4"/>
</Layout>
