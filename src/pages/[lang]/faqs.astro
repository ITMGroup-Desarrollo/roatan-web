---
import Layout from "../../layouts/Layout.astro";
import Header from "../../components/Headers/HeaderBlog.astro";
import Footer from "../../components/Footer/Footer.astro";
import Menu from "../../components/Headers/MenuBlog.astro";
import Faqss from "../../components/faqs/Faqs.astro";
export function getStaticPaths() {
  return [{ params: { lang: "es" } }, { params: { lang: "en" } }];
}
const lang = Astro.params.lang || "es";
---

<Layout lang={lang}>  
  <Menu lang={lang} />
  <Header
    lang={lang}
    headerTitle="Faq's"
    headerUrl="header-faqs.webp"
    id="headerFaqs-container"
  />
  <Faqss lang={lang} />
  <Footer lang={lang} videoFile="fondo-footer-gris.mp4"/>
</Layout>
